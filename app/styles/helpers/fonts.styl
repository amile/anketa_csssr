@font-face 
	font-family PT Sans
	src	url('/assets/fonts/PTSans/PTSans.eot')
	src local(PT Sans)
	src	url('/assets/fonts/PTSans/PTSans.woff') format('woff'),
		url('/assets/fonts/PTSans/PTSans.woff2') format('woff2'),
		url('/assets/fonts/PTSans/PTSans.eot?#iefix') format('embedded-opentype'),
		url('/assets/fonts/PTSans/PTSans.ttf') format('truetype'),
		url('/assets/fonts/PTSans/PTSans.svg#PTSans-Regular') format('svg')
	font-style normal
	font-weight 400

@font-face 
	font-family PT Sans
	src	url('/assets/fonts/PTSans/PTSans_bold.eot')
	src local(PT Sans Bold)
	src url('/assets/fonts/PTSans/PTSans_bold.woff') format('woff'),
		url('/assets/fonts/PTSans/PTSans_bold.woff2') format('woff2'),
		url('/assets/fonts/PTSans/PTSans_bold.eot?#iefix') format('embedded-opentype'),
		url('/assets/fonts/PTSans/PTSans_bold.ttf') format('truetype'),
		url('/assets/fonts/PTSans/PTSans_bold.svg#PTSans-Bold') format('svg')
	font-style normal
	font-weight 700